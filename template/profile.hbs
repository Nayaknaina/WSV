    var hash = window.location.hash;
    $('#nav-tab a[href="' + hash + '"]').tab('show');
  </script>

  <script>
    function updateProfileData(e) {
      let formInputs = document.querySelectorAll('#profile-form .input')
      let editBtn = document.querySelector('#profile-form #edit')
      let saveBtn = document.querySelector('#profile-form #save')
      if (editBtn.textContent === 'Edit') {
        editBtn.textContent = 'Cancel'
        saveBtn.disabled = false
        formInputs.forEach((forminput) => {
          forminput.disabled = false
        })
      } else {
        editBtn.textContent = 'Edit'
        saveBtn.disabled = true
        formInputs.forEach((forminput) => {
          forminput.disabled = true
        })
      }

    }
  </script>




