  <div class="container-fluid">
    <div class="row">
      {{> sidebar}}
      <div class="fg-main-content-wrap col-xl-11 col-lg-11 col-md-12 p-0 home-section">
        {{> NameHead}}

        {{#if successMSG}}
        <div class="col-sm-12">
          <div id="alert-success" class="alert  alert-success alert-dismissible fade show" role="alert">
            <span class="badge badge-pill badge-success">Success</span>
            {{#ifEquals successMSG 'free' }}
            Cannot add more than 3 team members with free plan
            <a href="/pricing">check out here.</a>
            {{else}}

            {{#ifEquals successMSG 'basic' }}
            Cannot add more than 7 team members with free plan
            <a href="/pricing">check out here.</a>
            {{else}}
            {{successMSG}}
            {{/ifEquals}}
            {{/ifEquals}}

            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
        </div>
        {{/if}}


        <div class="row col-11 ml-5">
          <div class="col-8"></div>
          <div class="col-3 mb-2 mt-2">
            <button type="button" class="btn btn-outline-primary font-15 main-btn-rect popup-btn"
              data-popup="popup-reg">
              <i class="fas fa-plus-circle font-12"></i>
              <span class="ml-1 d-none d-sm-inline">Add Team Member</span>
            </button>

          </div>
        </div>
        {{#if user.teams}}
        <table>
          <thead>
            <tr>
              <th scope="col">Name</th>
              <th scope="col">Email</th>
              <th scope="col">Mobile</th>
              <th scope="col">Actions</th>
            </tr>
          </thead>
          <tbody>
            {{#each user.teams}}
            <tr>
              {{!-- <td data-label="Account"><a href="/user/member/dashboard/{{_id}}">{{name}}</a></td> --}}
              <td data-label="Name"><a href="#">{{name}}</a></td>
              <td data-label="Email">{{email}}</td>
              <td data-label="Mobile">{{mobile}}</td>
              {{!-- <td data-label="Amount">{{myleads.length}}</td> --}}
              {{!-- <td><a href="/member/blocked/{{_id}}"><i class="fa-solid fa-ban" style="color: #ff0000;"></i></a>
              </td> --}}
              <td data-label="Actions">
                {{#if blocked}}
                <a href="/user/member/un-blocked/{{_id}}"><button type="button"
                    class="btn btn-success btn-sm">Un-Block</button></a>
                {{else}}
                <a href="/user/member/blocked/{{_id}}"><button type="button"
                    class="btn btn-danger btn-sm">Block</button></a>
                {{/if}}
              </td>
            </tr>
            {{/each }}

          </tbody>
        </table>
        {{else}}
        <h1 style="
      text-align: center;
      padding: 40px 70px;
    font-size: 30px !important;
    color: #36383a89;
      ">You dont have any team member.</h1>
        {{/if}}


      </div>
    </div>
